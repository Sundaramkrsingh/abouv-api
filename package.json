{
  "name": "bezt.pwa.api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"prisma/**/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:docker:build": "docker compose -f compose.dev.yaml up -d",
    "dev:docker:down": "docker compose -f compose.dev.yaml down --volumes",
    "dev:docker:down:images": "docker compose -f compose.dev.yaml down --volumes --rmi all",
    "dev:db:create": "pnpm dev:docker:build",
    "dev:db:remove": "pnpm dev:docker:down",
    "db:reset": "pnpm prisma migrate reset --force",
    "dev:db:seed": "ts-node prisma/seed.dev.ts",
    "dev:db:setupdata": "pnpm db:reset && pnpm prisma migrate deploy && pnpm dev:db:seed",
    "prod:db:seed": "ts-node prisma/seed.prod.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "{src,apps,libs,test}/**/*.ts": "eslint --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/s3-request-presigner": "^3.556.0",
    "@fontsource/baloo-2": "^5.0.20",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "^5.8.1",
    "@react-email/components": "^0.0.21",
    "@types/nodemailer": "^6.4.15",
    "axios": "^1.7.5",
    "bcrypt": "^5.1.1",
    "components": "link:@types/@react-email/components",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.10",
    "libphonenumber-js": "^1.10.61",
    "lodash.merge": "^4.6.2",
    "mime-types": "^2.1.35",
    "mixpanel": "^0.18.0",
    "nest-winston": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.3.1",
    "reflect-metadata": "^0.1.13",
    "run": "^1.5.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.merge": "^4.6.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/react": "^18.3.3",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "country-state-city": "^3.2.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.2",
    "lodash.isequal": "^4.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.x"
  }
}
